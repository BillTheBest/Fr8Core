<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <log4net xdt:Transform="Replace" configSource ="log4net.tests.healthMonitor.Release.config"/>
  
  <appSettings>
    <add key="HubApiBaseUrl" value="https://fr8.co/api/v1/" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>

    <add key="KeyVaultClientId" value="3abb93b7-0bd4-42f2-ae7b-82f91d098e72"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="KeyVaultUrl" value="https://fr8LiveKV.vault.azure.net:443"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <connectionStrings>
    <add name="DockyardDB" connectionString="Data Source=.;Initial Catalog=DockyardDB2;Integrated Security=SSPI;Transaction Binding=Explicit Unbind;" providerName="System.Data.SqlClient"  xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

  <system.diagnostics>
    <switches>
      <add name="ADALLegacySwitch" value="Warning"/>
    </switches>
  </system.diagnostics>
  
</configuration>