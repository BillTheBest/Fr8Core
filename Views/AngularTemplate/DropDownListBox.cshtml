@*RadioButtonGroup control main container*@
<ui-select ng-model="selectedItem" theme="bootstrap" search-enabled="false" on-select="setSelectedItem($item)" stop-click-propagation>
    <ui-select-match placeholder="Select value from the list...">{{$select.selected.key || select.selected.Key || $select.selected}}</ui-select-match>
    <ui-select-choices repeat="listItem in field.listItems">
        <span ng-bind-html="listItem.key || listItem.Key"></span>
    </ui-select-choices>
</ui-select>
