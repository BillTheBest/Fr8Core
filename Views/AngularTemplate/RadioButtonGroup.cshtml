@*RadioButtonGroup control main container*@
<div style="margin-top: 30px;">
    <span>Values for Demo -> {{field || json}}</span>
    <br />
    <label ng-repeat="radio in field.radios" ng-click="ChangeSelection(radio)">
        <input type="radio" name="{{field.groupName}}" ng-checked="radio.selected" ng-value="true" />
        {{radio.value}}
    </label>
</div>
