<input type="radio" ng-click="changeSelection(field)" name="pca__chk__{{groupName}}" id="pca__chk__{{groupName | validId}}__{{field.$$hashKey}}"
       ng-checked="{{field.selected}}" ng-value="true" stop-click-propagation/>
<label for="pca__chk__{{groupName | validId }}__{{field.$$hashKey}}" stop-click-propagation class="radio-button-option-label">
    {{field.value}}
</label>
@* Support nested controls *@
<div class="nested-controls">
</div>