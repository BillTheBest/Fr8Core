<body id="top">

    <!-- NAVBAR -->
    @Html.Partial("../Shared/_HomeNavNew", new HubWeb.ViewModels.NavLinksHome())
    <!-- END NAVBAR -->
    <!-- HERO UNIT -->
    <div id="hero-unit" class="hero-unit hero-light hero-video" data-vide-bg="mp4: Content/videos/motion.mp4, webm: Content/videos/motion.webm, ogv: Content/videos/motion.ogv, poster: Content/videos/motion.png" data-vide-options="loop: true, posterType: png">
        <div class="hero-content text-center">
            <div id="slidecaption" class="slide-caption">
                <div class="hero-text">
                    <h2 class="hero-heading">Welcome</h2>
                    <p>Our workflows simplify your life.</p>
                </div>
            </div>

            <div class="col-md-12">
                <div class="col-md-6">
                    <a href="~/Services/DocuSign"  style="float:right; padding-right:15px;">
                        <div class="one-connect demo-card-big-square demo-card-square-docusign-box mdl-card mdl-shadow--2dp" style="width: 550px; height: 400px;">
                            <div class="mdl-card__title mdl-card--expand" style="background-size:cover">
                                <h2 class="mdl-card__title-text">Featured Solution</h2>
                            </div>
                            <div class="mdl-card__supporting-text">
                                <li>Mail Merge from Excel or Google</li>
                                <li>Pull data out of DocuSign Envelopes</li>
                                <li>Create rich notifications to tell you what's signed and what's not signed</li>
                                <li>Choose DocuSign flows based on a spreadsheet value</li>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6">
                    <a href="~/dashboard/myaccount" style="float:left; padding-left:15px;">
                        <div class="demo-card-big-square demo-card-square-fr8-box mdl-card mdl-shadow--2dp"  style="width: 550px; height: 400px;">
                            <div class="mdl-card__title mdl-card--expand" style="background-size:cover">
                                <h2 class="mdl-card__title-text">Plan Builder</h2>
                            </div>
                            <div class="mdl-card__supporting-text">
                                <li>Build custom Fr8 Plans</li>
                                <li>Create powerful web workflows with a few mouse clicks</li>
                                <li>Create custom solutions</li>
                                <li>Import documents and process them</li>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    <div class="hero-bg-welcome-pattern"></div>
