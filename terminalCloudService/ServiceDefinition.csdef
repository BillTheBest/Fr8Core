<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="terminalCloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="terminalWebRole" vmsize="Small">
    <Sites>
      <Site name="pluginAzureSqlServer" physicalDirectory="..\..\..\pluginAzureSqlServer\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginAzureSqlServer" />
        </Bindings>
      </Site>
      <Site name="pluginSlack" physicalDirectory="..\..\..\pluginSlack\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginSlack" />
        </Bindings>
      </Site>
      <Site name="terminalDocuSign" physicalDirectory="..\..\..\terminalDocuSign\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="terminalDocuSign" />
        </Bindings>
      </Site>
      <Site name="pluginSalesforce" physicalDirectory="..\..\..\pluginSalesforce\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginSalesforce" />
        </Bindings>
      </Site>
      <Site name="terminalFr8Core" physicalDirectory="..\..\..\terminalFr8Core\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="terminalFr8Core" />
        </Bindings>
      </Site>
      <Site name="pluginSendGrid" physicalDirectory="..\..\..\pluginSendGrid\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginSendGrid" />
        </Bindings>
      </Site>
      <Site name="pluginTwilio" physicalDirectory="..\..\..\pluginTwilio\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginTwilio" />
        </Bindings>
      </Site>
      <Site name="pluginExcel" physicalDirectory="..\..\..\pluginExcel\azure.publish">
        <Bindings>
          <Binding name="Endpoint1" endpointName="pluginExcel" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="pluginAzureSqlServer" protocol="http" port="46281" />
      <InputEndpoint name="pluginSlack" protocol="http" port="39504" />
      <InputEndpoint name="terminalDocuSign" protocol="http" port="53234" />
      <InputEndpoint name="pluginSalesforce" protocol="http" port="51234" />
      <InputEndpoint name="terminalFr8Core" protocol="http" port="50705" />
      <InputEndpoint name="pluginSendGrid" protocol="http" port="10601" />
      <InputEndpoint name="pluginTwilio" protocol="http" port="30699" />
      <InputEndpoint name="pluginExcel" protocol="http" port="47011" />
    </Endpoints>
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
  </WebRole>
</ServiceDefinition>